/* pages/shop/shop.wxss */

.notice-image, .notice {
  width: 100%;
  height: 200rpx;
}

.shop-qr-note {
  text-align: center;
  font-size: 24rpx;
  color: #666;
  margin-top: 20rpx;
}

.store-phone .right {
  margin-left: 100rpx;
}

.store-phone {
  display: flex;
  color: #f80;
  font-size: 28rpx;
  align-items: center;
  justify-content: flex-start;
}

.store-phone::after {
  border-top-color: transparent !important;
}

.shop-qr-image {
  width: 200rpx;
  height: 200rpx;
}

.shop-detail-image-list {
  padding: 0 100rpx;
}

.shop-detail-image {
  width: 100rpx;
  height: 100rpx;
  margin-right: 10rpx;
}

.discount-item {
  border: 2rpx solid #f80;
  margin: 20rpx;
  display: flex;
  justify-content: flex-start;
  box-sizing: border-box;
}

.discount-input {
  height: 80rpx;
  line-height: 80rpx;
  width: 70%;
  padding-left: 20rpx;
}

.discount-btn {
  height: 80rpx;
  line-height: 80rpx;
  background-color: #f80;
  color: #fff;
  width: 30%;
  text-align: center;
}

.btn-item-active {
  background-color: #f80;
  color: #fff;
}

.btn-item-active .btn-tit {
  color: #fff;
}

.btn-item {
  padding: 15rpx 0;
  text-align: center;
  border-bottom: 2rpx solid #f80;
  width: 50%;
  box-sizing: border-box;
}

.btn-title {
  font-size: 28rpx;
}

.btn-tit {
  font-size: 24rpx;
}

.goods-category {
  display: flex;
  justify-content: flex-start;
}

.goods-category-list-fixed {
  position: fixed;
  top: 0;
}

.goods-category-list-view {
  width: 100%;
  height: 80rpx;
  overflow-y: scroll;
  position: relative;
  -webkit-overflow-scrolling: touch;
}

.goods-category-list-view::-webkit-scrollbar {
  display: none;
}

.goods-category-list {
  padding: 0 10rpx;
  width: max-content;
  min-width: 100%;
  background-color: #f80;
  padding-right: 50rpx;
}

.goods-category .right-icon {
  position: absolute;
  top: 1;
  right: 0;
  color: #fff;
  font-size: 40rpx;
  font-weight: bold;
  background-color: #f80;
  line-height: 80rpx;
  width: 50rpx;
  min-width: 50rpx;
  text-align: center;
  /* margin-top: 20rpx; */
}

.goods-category .right-icon-o {
  height: 30rpx;
  width: 30rpx;
  animation: twinkling 800ms infinite ease-out;
}

@-webkit-keyframes twinkling {
  10% {
    transform: translateX(5px);
  }

  100% {
    transform: translateX(0);
  }
}

.category-item {
  font-size: 28rpx;
  line-height: 80rpx;
  padding: 0 20rpx;
  color: #fff;
}

.category-item-active {
  font-size: 30rpx;
  font-weight: bold;
}

.goods-list {
  width: 100%;
}

.shop {
  width: 100%;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee;
  padding: 6rpx 0 0;
  min-height: 120rpx;
}

.shop-detail-left {
  width: 80%;
  display: flex;
  /* justify-content: space-evenly; */
  align-items: center;
  margin-left: 20rpx
}

.shop-detail-right {
  margin-right: 20rpx;
}

.shop-qualification-image {
  min-width: 100rpx;
}

.shop-qualification-image .img {
  min-width: 120rpx;
  width: 120rpx;
  height: 90rpx;
  border-radius: 5rpx;
  overflow: hidden;
}

.shop-logo-image {
  margin: 5rpx;
  margin-right: 10rpx;
  font-size: 0;
  position: relative;
}

.shop-logo-image .state {
  position: absolute;
  left: 0;
  bottom: 0;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.7);
  height: 40rpx;
  font-size: 24rpx;
  text-align: center;
  width: 100rpx;
  line-height: 40rpx;
}

.shop-logo-image .shop-image {
  /* min-width: 120rpx;
  min-height: 120rpx; */
  min-width: 100rpx;
  min-height: 100rpx;
  box-sizing: border-box;
  border-radius: 5rpx;
  overflow: hidden;
}

.shop .detail-right {
  max-width: 460rpx;
}

.shop .shop-name {
  /* margin-left: 10rpx; */
  font-size: 32rpx;
  font-weight: bold;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  margin-bottom: 10rpx
}

.pickup,.addr{
  /* width: 100%; */
  font-size: 20rpx;
  color: #FDAD00;
}

.addr{
  width: 482rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.pickup>text:first-child{
  margin-right: 20rpx;
  box-sizing: border-box;
  max-width: 265rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
