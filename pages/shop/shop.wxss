/* 团购商品 */
.group-list{
  background-color: rgba(0, 0, 0, .06);
  padding-top:30rpx
}

.goods{
  background-color: #fff;
  width: 98%;
  height: 760rpx;
  margin: 0 auto;
  border-radius: 8rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}

view.img{
  position: relative;
  height: 550rpx;
}

view.img>image{
  width: 100%;
  height: 550rpx;
  border-radius: 16rpx;
}

.look{
  width: 220rpx;
  height: 60rpx;
  background-color: rgb(216, 178, 135);
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 8rpx;
  position: absolute;
  right: 0rpx;
  bottom: 0rpx;
  letter-spacing: 2rpx;
}

.item{
  display: flex;
  flex-wrap: wrap;
  padding: 5rpx 10rpx;
  justify-content: space-between;
  letter-spacing: 2rpx;
  height: 210rpx;
  box-sizing: border-box;
}

.item-left,.item-right{
  width: 66%;
  height: 200rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
}

.item-right{
  width: 34%;
  text-align: center;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-end;
}

.good-name{
  font-size: 32rpx;
  color: #000;
  font-weight: 700;
  letter-spacing: 2rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-top: 20rpx;
  box-sizing: border-box;
}

.item-price{
  display: flex;
  margin-top: 10rpx;
  box-sizing: border-box;
}

.price{
  text-decoration:line-through;
  color: #999;
  font-size: 28rpx;
  margin-right: 20rpx;
  box-sizing: border-box;
}

.couponRate{
  font-size: 34rpx;
  font-weight: 700;
  color: #E14646;
}

.time{
  font-size: 29rpx;
  color: #fdad00;
  /* font-weight: 600; */
  margin-top: 30rpx;
  box-sizing: border-box;
}

.soldSize{
  width: 230rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #000;
  /* text-align: left; */
  margin-top: -10rpx;
  box-sizing: border-box;
}

.stockSize{
  width: 230rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #fdad00;
  /* text-align: left; */
  /* margin-top: 5rpx; */
  box-sizing: border-box;
}

.add{
  width: 230rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  background-color: #E14646;
  color: #fff;
  font-size: 40rpx;
  font-weight: 700;
  margin-top: 20rpx;
  box-sizing: border-box;
}

.getAddress{
  border: 1rpx solid #ddd;
  border-radius: 8rpx;
  text-align: right;
  font-size: 24rpx;
  color: #f80;
  position: relative;
  font-weight: 600;
  width: 350rpx;
  height: 50rpx;
  line-height: 50rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
}

.getAddress image{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  left: 8rpx;
}

.shop-item{
  width: 100%;
  padding: 10px 15px;
  font-size: 14px;
  color: #333;
  box-sizing: border-box;
  display: flex;
  /* justify-content: space-between; */
}

.shop-item-addr{
  width: 90px;
}

.shop-item text{
  color: #999;
  width: 240px;
  /* padding-left: 20rpx; */
  box-sizing: border-box;
}