/* pages/cashRoll/cashRoll.wxss */

page {
  padding: 0 20rpx;
  box-sizing: border-box;
}

.none{
  width: 100%;
  height: 400rpx;
  text-align: center;
  line-height: 400rpx;
}

.cashRoll {
  background-color: #92D050;
  padding: 0 20rpx;
  border-radius: 20rpx;
  margin: 20rpx 0;
  box-shadow: 0 0 5rpx 2rpx #eee;
  display: flex;
  justify-content: space-between;
  align-content: center;
  align-items: center
}

.type{
  font-size: 32rpx;
  font-weight: bold;
  width: 40rpx;
  margin-left: 20rpx
}

.time{
  font-size: 28rpx;
  color: #fff;
  margin-top: 10rpx
}

.cashRoll-shopName {
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  
}

.custCoupons-price {
  color: #fff;
  font-size: 30rpx;
  font-weight: 600;
  text-align: center;
  margin-top: 18rpx;
  box-sizing: border-box
}

.cashRoll-price {
  color: #ED0000;
  font-size: 30rpx;
  font-weight: 600;
  text-align: center;
  margin-top: 18rpx;
  box-sizing: border-box
}
.custCoupons{
  background-color: #FF8900;
  padding: 0 30rpx;
  border-radius: 20rpx;
  margin: 20rpx 0;
  box-shadow: 0 0 5rpx 2rpx #eee;
  display: flex;
  justify-content: space-between;
  align-content: center;
  align-items: center;
  height: 140rpx;
}

.center{
  width: 350rpx;
  height: 100%;
  padding: 10rpx;
  box-sizing: border-box;
  border-left: 2rpx #fff dashed
}

.right{
  font-weight: 600;
  font-size: 36rpx;
  height: 100%;
  padding: 10rpx 0 10rpx 10rpx;
  box-sizing: border-box;
  border-left: 2rpx #fff dashed
}

.right .time{
  font-size: 30rpx;
  margin-top: 18rpx;
}